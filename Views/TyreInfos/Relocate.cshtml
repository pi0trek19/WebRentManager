@model  TyreInfoRelocateViewModel

    <div class="container">
        <form enctype="multipart/form-data" asp-action="relocate" asp-controller="tyreinfos" method="post">
            <div class="form-group row">
                <label asp-for="NewTyreShopId" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-10">
                    <select asp-for="NewTyreShopId" class="custom-select mr-sm-2"
                            asp-items="@(new SelectList(Model.TyreShops,"Id","Name"))"></select>
                </div>
            </div>
            <input hidden asp-for="Id" value="@Model.Id" />
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
        <div>
            <a asp-action="details" asp-controller="cars" asp-route-id="@Model.CarId" class="btn btn-danger">Anuluj</a>
        </div>
    </div>
