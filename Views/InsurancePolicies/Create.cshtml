@model WebRentManager.ViewModels.InsurancePolicyCreateViewModel

<br />
<form asp-action="Create" enctype="multipart/form-data" method="post">
    <div class="form-group row">
        <label asp-for="ValidFrom" class="col-sm-2 control-label"></label>
        <div class="col-sm-10">
            <input type="date" value="null" asp-for="ValidFrom" class="form-control" />
            <span asp-validation-for="ValidFrom" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="ValidTo" class="col-sm-2 control-label"></label>
        <div class="col-sm-10">
            <input type="date" value="null" asp-for="ValidTo" class="form-control" />
            <span asp-validation-for="ValidTo" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="Cost" class="col-sm-2 control-label"></label>
        <div class="col-sm-10">
            <input asp-for="Cost" class="form-control" />
            <span asp-validation-for="Cost" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="Number" class="col-sm-2 control-label"></label>
        <div class="col-sm-10">
            <input asp-for="Number" class="form-control" />
            <span asp-validation-for="Number" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="InsuranceCompany" class="col-sm-2 control-label"></label>
        <div class="col-sm-10">
            <input asp-for="InsuranceCompany" class="form-control" />
            <span asp-validation-for="InsuranceCompany" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group">
        <input type="submit" value="Dodaj" class="btn btn-primary" />
    </div>
</form>
