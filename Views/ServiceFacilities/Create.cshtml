@model ServiceFacilityCreateViewModel

    <div>
        <form enctype="multipart/form-data" asp-controller="ServiceFacilities" asp-action="create" method="post">
            <div class="form-group row">
                <label asp-for="Name" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-10">
                    <input asp-for="Name" class="form-control" placeholder="Nazwa" />
                    <span asp-validation-for="Name" class="text-danger"></span>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="Street" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-10">
                    <input asp-for="Street" class="form-control" placeholder="Adres" />
                    <span asp-validation-for="Street" class="text-danger"></span>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="ZipCode" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-10">
                    <input asp-for="ZipCode" class="form-control" placeholder="Kod pocztowy" />
                    <span asp-validation-for="ZipCode" class="text-danger"></span>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="City" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-10">
                    <input asp-for="City" class="form-control" placeholder="Miasto" />
                    <span asp-validation-for="City" class="text-danger"></span>
                </div>
            </div>
            <div class="form-group-row">
                <button type="submit" class="btn btn-primary">Dodaj</button>
            </div>
        </form>
    </div>
